{
    "name": "hyperf/swoole-dashboard",
    "description": "A swoole dashboard library for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "swoole-dashboard"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.3",
        "hyperf/contract": "~1.0.0",
        "hyperf/utils": "~1.0.0",
        "psr/container": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9",
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "suggest": {
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\SwooleDashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\SwooleDashboard\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
